<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox id="csGUI" fx:id="musicPane" maxHeight="-Infinity" prefHeight="100.0" prefWidth="1190.0" spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="choreography.view.music.MusicPaneController">
  <children>
    <VBox id="VBox" alignment="CENTER" fillWidth="true" spacing="2.0">
      <children>
        <HBox id="HBox" alignment="CENTER" spacing="5.0">
          <children>
            <!--                 <Button mnemonicParsing="false" onAction="#previousSong" text="&lt;&lt;" /> -->
            <Button disable="false" minWidth="-Infinity" mnemonicParsing="false" onAction="#playSong" prefWidth="37.0" text="Play">
              <font>
                <Font size="10.0" fx:id="x3" />
              </font>
              <HBox.margin>
                <Insets left="2.0" right="2.0" top="2.0" fx:id="x2" />
              </HBox.margin>
            </Button>
            <Button font="$x3" minWidth="-Infinity" mnemonicParsing="false" onAction="#pauseSong" prefWidth="45.0" text="Pause" HBox.margin="$x2" />
            <ScrollPane fx:id="waterTimeline" content="$null" hbarPolicy="NEVER" hmax="100.0" minHeight="-Infinity" prefHeight="29.0" prefWidth="1190.0" vbarPolicy="NEVER">
              <HBox.margin>
                <Insets left="5.0" right="3.0" />
              </HBox.margin>
            </ScrollPane>
          </children>
        </HBox>
        <HBox id="HBox" alignment="CENTER" spacing="0.0">
          <children>
            <Label fx:id="songName" alignment="CENTER" maxWidth="-1.0" minWidth="-Infinity" prefWidth="105.0" text="No Music Selected" textAlignment="LEFT" HBox.hgrow="ALWAYS">
              <font>
                <Font name="System Bold Italic" size="10.0" />
              </font>
              <HBox.margin>
                <Insets top="5.0" fx:id="x5" />
              </HBox.margin>
            </Label>
            <ScrollPane fx:id="timeLabel" hmax="100.0" minHeight="29.0" prefHeight="29.0" prefViewportHeight="29.0" prefViewportWidth="1082.0" prefWidth="1190.0" vbarPolicy="NEVER">
              <content>
                <LineChart fx:id="labelChart" animated="false" legendVisible="false" minHeight="25.0" prefHeight="11.0" prefWidth="2001.0">
                  <xAxis>
                    <NumberAxis fx:id="labelAxis" animated="false" autoRanging="false" maxWidth="-Infinity" minWidth="-Infinity" minorTickCount="4" minorTickLength="2.0" minorTickVisible="true" prefHeight="18.0" prefWidth="1963.9999000000025" side="BOTTOM" tickLabelGap="2.0" tickLength="5.0" tickMarkVisible="true" tickUnit="10.0" upperBound="2000.0" />
                  </xAxis>
                  <yAxis>
                    <NumberAxis animated="false" autoRanging="false" minorTickCount="0" minorTickLength="0.0" prefHeight="29.0" side="TOP" tickLabelGap="0.0" tickLength="0.0" tickMarkVisible="false" tickUnit="0.0" upperBound="0.0" />
                  </yAxis>
                  <padding>
                    <Insets fx:id="x1" />
                  </padding>
                </LineChart>
              </content>
              <HBox.margin>
                <Insets right="3.0" />
              </HBox.margin>
            </ScrollPane>
          </children>
        </HBox>
      </children>
      <padding>
        <Insets fx:id="x1" />
      </padding>
      <VBox.margin>
        <Insets bottom="2.0" fx:id="x4" />
      </VBox.margin>
    </VBox>
    <HBox fillHeight="false" minHeight="-Infinity" prefHeight="29.0" prefWidth="1190.0" spacing="0.0">
      <children>
        <Label fx:id="songProgress" alignment="CENTER_LEFT" contentDisplay="LEFT" labelFor="$songProgress" minWidth="-Infinity" prefWidth="40.0" text="0.0" HBox.hgrow="SOMETIMES">
          <HBox.margin>
            <Insets left="10.0" />
          </HBox.margin>
        </Label>
        <Button font="$x3" minWidth="-Infinity" mnemonicParsing="false" onAction="#stopSong" text="Reset" HBox.hgrow="NEVER" HBox.margin="$x2" />
        <Slider id="time" fx:id="timeSlider" majorTickUnit="0.5" max="100.0" minorTickCount="4" prefHeight="19.0" prefWidth="1091.0" showTickLabels="false" showTickMarks="false">
          <HBox.margin>
            <Insets left="8.0" />
          </HBox.margin>
        </Slider>
      </children>
      <padding>
        <Insets fx:id="x1" />
      </padding>
    </HBox>
  </children>
</VBox>
