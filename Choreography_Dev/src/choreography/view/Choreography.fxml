<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.input.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="csGUI" prefHeight="400.0" prefWidth="1190.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="choreography.view.ChoreographyController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File" fx:id="fileMenu">
          <items>
            <MenuItem mnemonicParsing="false" text="New" fx:id="newItemMenuItem">
              <accelerator>
                <KeyCodeCombination alt="UP" code="N" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
              </accelerator>
            </MenuItem>
            <MenuItem mnemonicParsing="false" text="Open…" fx:id="openWizardMenuItem">
              <accelerator>
                <KeyCodeCombination alt="UP" code="O" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
              </accelerator>
            </MenuItem>
            <Menu mnemonicParsing="false" text="Open Recent" fx:id="openRecentMenuItemItem" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Close" fx:id="closeMenuItem" />
            <MenuItem mnemonicParsing="false" text="Save" fx:id="saveMenuItem">
              <accelerator>
                <KeyCodeCombination alt="UP" code="S" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
              </accelerator>
            </MenuItem>
            <MenuItem mnemonicParsing="false" text="Save As…" fx:id="saveAsMenuItem">
              <accelerator>
                <KeyCodeCombination alt="UP" code="S" control="DOWN" meta="UP" shift="DOWN" shortcut="UP" />
              </accelerator>
            </MenuItem>
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" fx:id="preferencesMenuItem">
              <accelerator>
                <KeyCodeCombination alt="UP" code="COMMA" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
              </accelerator>
            </MenuItem>
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" fx:id="quitMenuItem">
              <accelerator>
                <KeyCodeCombination alt="UP" code="Q" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
              </accelerator>
            </MenuItem>
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit" fx:id="editMenu">
          <items>
            <MenuItem mnemonicParsing="false" text="Undo" fx:id="undoMenuItem">
              <accelerator>
                <KeyCodeCombination alt="UP" code="Z" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
              </accelerator>
            </MenuItem>
            <MenuItem mnemonicParsing="false" text="Redo" fx:id="redoMenuItem">
              <accelerator>
                <KeyCodeCombination alt="UP" code="Z" control="DOWN" meta="UP" shift="DOWN" shortcut="UP" />
              </accelerator>
            </MenuItem>
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Cut" fx:id="cutMenuItem">
              <accelerator>
                <KeyCodeCombination alt="UP" code="X" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
              </accelerator>
            </MenuItem>
            <MenuItem mnemonicParsing="false" text="Copy" fx:id="copyMenuItem">
              <accelerator>
                <KeyCodeCombination alt="UP" code="C" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
              </accelerator>
            </MenuItem>
            <MenuItem mnemonicParsing="false" text="Paste" fx:id="pasteMenuItem">
              <accelerator>
                <KeyCodeCombination alt="UP" code="V" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
              </accelerator>
            </MenuItem>
            <MenuItem mnemonicParsing="false" text="Delete" fx:id="deleteMenuItem">
              <accelerator>
                <KeyCodeCombination alt="UP" code="D" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
              </accelerator>
            </MenuItem>
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help" fx:id="helpMenu">
          <items>
            <MenuItem mnemonicParsing="false" text="About" fx:id="aboutMenuItem" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <BorderPane prefHeight="376.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
      <bottom>
        <AnchorPane prefHeight="34.0" prefWidth="640.0" BorderPane.alignment="CENTER">
          <children>
            <Label fx:id="fcwOutput" alignment="CENTER" text="FCW Output" AnchorPane.bottomAnchor="9.0" AnchorPane.leftAnchor="14.0" />
            <fx:include source="colorPalette/ColorPalette.fxml" layoutY="0.0" prefHeight="34.0" prefWidth="800.0" AnchorPane.rightAnchor="0.0" />
          </children>
        </AnchorPane>
      </bottom>
      <center>
        <VBox prefHeight="200.0" prefWidth="100.0">
          <children>
            <fx:include source="sliders/Sliders.fxml" fillHeight="true">
              <padding>
                <Insets left="10.0" right="10.0" fx:id="x1" />
              </padding>
            </fx:include>
            <fx:include source="timeline/Timeline.fxml" padding="$x1" VBox.vgrow="ALWAYS">
              <VBox.margin>
                <Insets bottom="2.0" left="2.0" right="2.0" top="10.0" />
              </VBox.margin>
            </fx:include>
          </children>
        </VBox>
      </center>
      <top>
        <fx:include source="MusicPane.fxml" />
      </top>
    </BorderPane>
  </children>
</VBox>
